<structure name="pipeline">
    <structure name="input">
        <field minOccurs="0" name="connectionId" type="java.lang.String"/>
        <field minOccurs="0" name="transactionId" type="java.lang.String"/>
        <field name="alias" type="java.lang.String"/>
        <field name="realm" type="java.lang.String"/>
        <field minOccurs="0" name="maxUses" type="java.lang.Long"/>
        <field minOccurs="0" name="until" timezone="UTC" type="java.util.Date"/>
    </structure>
    <structure name="output">
        <field name="id" pattern="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}|[0-9a-fA-F]{32}" type="java.util.UUID"/>
        <field name="secret" type="java.lang.String"/>
    </structure>
    <structure collectionName="temporaryAuthentications" definition="nabu.authentication.temporary.types.database.temporaryAuthentication" name="temporary"/>
</structure>
